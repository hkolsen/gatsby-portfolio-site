{
  "name": "@hkolsen/gatsby-portfolio-site",
  "version": "1.0.0",
  "private": true,
  "description": "Static site for Heidi Olsen portfolio",
  "scripts": {
    "prebuild": "npm run clean && npm run type-check && npm run lint && npm run test",
    "build": "gatsby build",
    "clean": "rimraf .cache public",
    "develop": "npm run clean && gatsby develop",
    "format": "prettier --write '{gatsby-*.js,src/**/*.{md,json,ts,tsx},static/**/*.yml,*.{md,json}}'",
    "gatsby-info": "gatsby info --clipboard",
    "lint": "eslint --ext ts,tsx src/",
    "lint:watch": "eslint --ext ts,tsx src/ --watch",
    "serve": "gatsby serve",
    "start": "npm run develop",
    "test": "jest --passWithNoTests",
    "type-check": "tsc --noEmit"
  },
  "eslintIgnore": [
    "node_modules",
    "output",
    "translationTemplates",
    "__generated__",
    "__snapshots__",
    "dist",
    "build",
    "bundle"
  ],
  "dependencies": {
    "gatsby": "^2.0.66",
    "gatsby-background-image": "^0.8.3",
    "gatsby-image": "^2.0.23",
    "gatsby-plugin-module-resolver": "^1.0.3",
    "gatsby-plugin-netlify": "^2.0.15",
    "gatsby-plugin-netlify-cms": "^3.0.18",
    "gatsby-plugin-react-helmet": "^3.0.4",
    "gatsby-plugin-react-svg": "^2.1.2",
    "gatsby-plugin-sharp": "^2.0.35",
    "gatsby-plugin-styled-components": "^3.0.6",
    "gatsby-plugin-typescript": "^2.0.13",
    "gatsby-remark-images": "^3.0.11",
    "gatsby-remark-relative-images": "^0.2.1",
    "gatsby-source-filesystem": "^2.0.31",
    "gatsby-transformer-remark": "^2.3.10",
    "gatsby-transformer-sharp": "^2.1.18",
    "intersection-observer": "^0.6.0",
    "lodash": "^4.17.5",
    "lodash-webpack-plugin": "^0.11.4",
    "netlify-cms": "^2.9.1",
    "react": "^16.2.0",
    "react-helmet": "^5.2.0",
    "react-icon-base": "^2.1.2",
    "react-icons": "^3.7.0",
    "react-intl": "^2.8.0",
    "react-is": "^16.8.6",
    "react-lottie": "^1.2.3",
    "react-motion": "^0.5.2",
    "react-reveal": "^1.2.2",
    "react-share": "^3.0.0",
    "react-tooltip-lite": "^1.9.4",
    "react-waypoint": "^9.0.2",
    "sharp": "^0.22.1",
    "showdown": "^1.9.0",
    "smooth-scroll": "^16.0.3",
    "styled-components": "^4.1.3",
    "uuid": "^3.2.1",
    "whatwg-fetch": "^3.0.0"
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.4.0",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@types/escape-string-regexp": "^2.0.1",
    "@types/graphql": "^14.0.5",
    "@types/jest": "^24.0.6",
    "@types/lodash": "^4.14.119",
    "@types/node": "^11.13.7",
    "@types/prettier": "^1.16.3",
    "@types/prop-types": "^15.7.1",
    "@types/qs": "^6.5.3",
    "@types/react": "^16.8.14",
    "@types/react-dom": "^16.8.4",
    "@types/react-helmet": "^5.0.7",
    "@types/react-icon-base": "^2.1.3",
    "@types/react-intl": "^3.0.0",
    "@types/react-is": "^16.7.1",
    "@types/react-lottie": "^1.2.2",
    "@types/react-motion": "^0.0.29",
    "@types/react-share": "^2.1.1",
    "@types/showdown": "^1.9.2",
    "@types/styled-components": "4.1.10",
    "@typescript-eslint/parser": "^2.1.0",
    "eslint": "^6.1.0",
    "graphql": "^14.1.1",
    "jest": "^24.7.1",
    "polished": "^3.0.3",
    "prettier": "^1.18.2",
    "react-testing-library": "^6.1.2",
    "rimraf": "^2.6.2",
    "typescript": "^3.5.0-rc"
  },
  "resolutions": {
    "eslint": "^6.1.0"
  }
}
